perl ../knotAnnotSV.pl --annotSVfile ./example.annotated.tsv --configFile ../config_AnnotSV.yaml

perl ../knotAnnotSV2XL.pl --annotSVfile ./example.annotated.tsv --configFile ../config_AnnotSV.yaml  --vbaBin ../vbaProject.bin



